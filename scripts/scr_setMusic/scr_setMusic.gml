function scr_getMusic(_zone){
var _music
switch _zone{
	case 1:
	_music=Moonlight_Shadow
	break;
	
	case 2:
	_music=Nightmare
	break;
	
	case 3:
	_music=The_Fairy_Academy
	break;
	
	case 4:
	_music=Zakum_Final_Fight
	break;
	
	case 5:
	_music=Minar_s_Dream
	break;
	
	case 6:
	_music=Destruction_Perion
	
	break;
	
	}
	return _music
}